{
  "name": "expense-tracker",
  "version": "0.0.0",
  "private": true,
  "description": "Expense tracker made with Redux",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leosuncin/expense-tracker.git"
  },
  "author": "Jaime Leonardo Suncin Cruz <leosuncin@gmail.com>",
  "homepage": "https://github.com/leosuncin/expense-tracker#readme",
  "bugs": {
    "url": "https://github.com/leosuncin/expense-tracker/issues"
  },
  "scripts": {
    "build": "next build",
    "cy:open": "cypress open",
    "debug": "NODE_OPTIONS='--inspect-brk' next",
    "dev": "next",
    "fixtures:load": "mongodb-fixtures load -u $MONGO_URL",
    "fixtures:rebuild": "mongodb-fixtures rebuild -u $MONGO_URL",
    "fixtures:unload": "mongodb-fixtures unload -u $MONGO_URL",
    "lint": "xo",
    "postinstall": "is-ci || husky install",
    "start": "next start",
    "start:e2e": "NODE_ENV=test start-test dev 3000 test:e2e",
    "test": "jest",
    "test:e2e": "cypress run",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit --noErrorTruncation --pretty false --incremental false"
  },
  "keywords": [
    "Next.js",
    "React",
    "Redux",
    "RTK",
    "MongoDB"
  ],
  "dependencies": {
    "@hookform/resolvers": "^2.4.0",
    "@reduxjs/toolkit": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "connected-next-router": "^4.0.0",
    "http-status-codes": "^2.1.4",
    "mongoose": "^5.12.5",
    "mongoose-unique-validator": "^2.0.3",
    "next": "^12.0.4",
    "next-connect": "^0.11.0",
    "next-iron-session": "^4.1.0",
    "next-redux-wrapper": "^7.0.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-hook-form": "^7.2.0",
    "react-redux": "^7.2.0",
    "zod": "^3.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@cypress/code-coverage": "^3.9.4",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@shelf/jest-mongodb": "^2.0.1",
    "@testing-library/cypress": "^8.0.0",
    "@testing-library/jest-dom": "^5.0.0",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/faker": "^5.5.0",
    "@types/jest": "^27.0.1",
    "@types/mongoose": "^5.10.5",
    "@types/mongoose-unique-validator": "^1.0.4",
    "@types/node": "^16.0.0",
    "@types/node-mongodb-fixtures": "^3.2.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.0",
    "@types/supertest": "^2.0.0",
    "babel-plugin-istanbul": "^6.0.0",
    "cypress": "^8.1.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-deprecation": "^1.2.0",
    "eslint-plugin-jest": "^25.2.2",
    "eslint-plugin-jest-formatting": "^3.0.0",
    "eslint-plugin-testing-library": "^5.0.0",
    "factory.ts": "^0.5.0",
    "faker": "^5.5.0",
    "fast-check": "^2.14.0",
    "husky": "^7.0.0",
    "is-ci": "^3.0.0",
    "jest": "^26.0.0",
    "jest-css-modules-transform": "^4.2.0",
    "jest-fetch-mock": "^3.0.0",
    "lint-staged": "^12.0.2",
    "node-mongodb-fixtures": "^3.2.0",
    "nyc": "^15.1.0",
    "prettier": "^2.2.0",
    "prettier-plugin-package": "^1.3.0",
    "prettier-plugin-sorted": "^2.0.0",
    "start-server-and-test": "^1.12.1",
    "supertest": "^6.1.0",
    "ts-jest": "^26.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.2.0",
    "xo": "^0.44.0"
  }
